# $Id: .htaccess 95 2010-04-12 12:41:49Z hjakison $

RewriteEngine On

#ErrorDocument 404 /error/404.html
#ErrorDocument 500 /error/500.html

# Replace XXX below with the subversion project name such that /newticket goes to
# project's new ticket page in Trac and /trac goes to the main Trac page.

RewriteRule ^newticket$ http://trac.altogetherhosting.com/XXX/newticket [R,L]
RewriteRule ^trac http://trac.altogetherhosting.com/XXX/ [R,L]
RewriteRule ^svn http://svn.altogetherhosting.com/XXX/ [R,L]
RewriteRule ^wiki http://wicked.dowcarter.com/twiki/bin/view/DCInteract/XXX [R,L]

#run everything through index page except for files and directories
RewriteCond %{SCRIPT_FILENAME} !-d
RewriteCond %{SCRIPT_FILENAME} !-f
#RewriteRule .* index.html [L] 
RewriteRule ^calculator calculator.html [L] 
RewriteRule ^login calculator.html [L] 
RewriteRule ^user calculator.html [L] 
RewriteRule ^game calculator.html [L]
RewriteRule ^menu calculator.html [L]
RewriteRule ^home calculator.html [L]
RewriteRule ^character calculator.html [L]
RewriteRule ^goals calculator.html [L]
RewriteRule ^signup calculator.html [L]
RewriteRule ^ajax calculator.html [L]
RewriteRule ^secretquestion calculator.html [L]
RewriteRule ^forgot calculator.html [L]
AddType application/x-httpd-php .html

# 403 Forbidden for any URL that includes .svn
RewriteRule \.svn / [F]